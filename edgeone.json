{
  "version": "1.0",
  "routes": [
    {
      "src": "/.*",
      "middleware": "./functions/_middleware.js",
      "method": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "*",
        "Access-Control-Allow-Headers": "*"
      }
    }
  ],
  "static": {
    "dir": "./",
    "exclude": [".git", ".edgeone"]
  },
  "functions": {
    "dir": "./functions",
    "runtime": "Nodejs18"
  }
}
